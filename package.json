{
    "name": "plex-sdk",
    "version": "2.0.1",
    "description": "Typescript SDK for Plex and Plex Media Server APIs",
    "repository": "git@github.com:nass600/plex-sdk",
    "license": "MIT",
    "author": "Ignacio Velazquez <ivelazquez85@gmail.com>",
    "main": "dist/plex-sdk.js",
    "exports": {
        ".": {
            "types": "./dist/plex-sdk.d.ts",
            "require": "./dist/plex-sdk.js",
            "import": "./dist/esm/plex-sdk.js"
        }
    },
    "module": "dist/esm/plex.js",
    "types": "dist/plex-sdk.d.ts",
    "scripts": {
        "prebuild": "npm run type-check",
        "build": "rollup -c --configPlugin typescript --bundleConfigAsCjs",
        "docs": "typedoc",
        "prelint": "npm run type-check",
        "lint": "npm run lint:js && npm run lint:md",
        "lint:js": "eslint . --ext ts,js --fix",
        "lint:md": "remark ./*.md -o",
        "prepare": "husky install",
        "prepublishOnly": "npm run build && npm run docs",
        "test": "jest --config jest.config.ts",
        "type-check": "tsc --noEmit",
        "postversion": "npm run lint:md"
    },
    "config": {
        "commitizen": {
            "path": "cz-conventional-changelog"
        }
    },
    "dependencies": {
        "axios": "^1.2.2"
    },
    "devDependencies": {
        "@babel/core": "7.7.7",
        "@babel/preset-env": "7.7.7",
        "@commitlint/cli": "^17.4.0",
        "@commitlint/config-conventional": "^17.4.0",
        "@rollup/plugin-babel": "^6.0.3",
        "@rollup/plugin-commonjs": "^24.0.0",
        "@rollup/plugin-json": "^6.0.0",
        "@rollup/plugin-node-resolve": "^15.0.1",
        "@rollup/plugin-terser": "^0.3.0",
        "@rollup/plugin-typescript": "^11.0.0",
        "@semantic-release/changelog": "^6.0.2",
        "@semantic-release/git": "^10.0.1",
        "@types/jest": "24.0.25",
        "@typescript-eslint/eslint-plugin": "^5.48.0",
        "@typescript-eslint/parser": "^5.48.0",
        "@zerollup/ts-transform-paths": "^1.7.18",
        "axios-mock-adapter": "1.17.0",
        "commitizen": "^4.2.6",
        "cz-conventional-changelog": "^3.3.0",
        "husky": "^8.0.3",
        "jest": "^29.3.1",
        "jest-watch-typeahead": "^2.2.1",
        "prettier": "2.1.1",
        "remark-cli": "^11.0.0",
        "rollup": "^3.9.1",
        "rollup-plugin-auto-external": "^2.0.0",
        "rollup-plugin-clear": "2.0.7",
        "semantic-release": "^20.0.0",
        "ts-jest": "^29.0.3",
        "ts-node": "^10.9.1",
        "typedoc": "^0.23.24",
        "typescript": "^4.9.4"
    },
    "engines": {
        "npm": ">=8.19.2",
        "node": ">=18.12.1"
    }
}
