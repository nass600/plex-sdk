{
  "name": "plex-sdk",
  "version": "4.0.0",
  "type": "module",
  "description": "Typescript SDK for Plex and Plex Media Server APIs",
  "repository": "git@github.com:nass600/plex-sdk",
  "license": "MIT",
  "author": "Ignacio Velazquez <ivelazquez85@gmail.com>",
  "exports": {
    ".": {
      "types": "./dist/index.d.ts",
      "import": "./dist/index.js"
    }
  },
  "module": "dist/index.js",
  "types": "dist/index.d.ts",
  "scripts": {
    "build": "rollup -c --configPlugin typescript --bundleConfigAsCjs",
    "prelint": "npm run type-check",
    "lint": "eslint . --ext ts,js --fix",
    "format": "prettier --write . --list-different",
    "prepare": "husky",
    "prepublishOnly": "npm run build",
    "test": "vitest run --coverage",
    "type-check": "tsc --noEmit"
  },
  "config": {
    "commitizen": {
      "path": "cz-conventional-changelog"
    }
  },
  "dependencies": {
    "normalize-text": "^2.6.0",
    "query-string": "^9.2.2"
  },
  "devDependencies": {
    "@commitlint/cli": "^19.8.1",
    "@commitlint/config-conventional": "^19.8.1",
    "@eslint/js": "^9.32.0",
    "@rollup/plugin-json": "^6.1.0",
    "@rollup/plugin-node-resolve": "^16.0.1",
    "@rollup/plugin-terser": "^0.4.4",
    "@rollup/plugin-typescript": "^12.1.4",
    "@semantic-release/changelog": "^6.0.2",
    "@semantic-release/git": "^10.0.1",
    "@typescript-eslint/eslint-plugin": "^8.38.0",
    "@typescript-eslint/parser": "^8.38.0",
    "@vitest/coverage-v8": "^3.2.4",
    "commitizen": "^4.2.6",
    "cz-conventional-changelog": "^3.3.0",
    "eslint": "^9.32.0",
    "eslint-config-prettier": "^10.1.8",
    "eslint-plugin-prettier": "^5.5.3",
    "eslint-plugin-unused-imports": "^4.1.4",
    "husky": "^9.1.7",
    "msw": "^2.0.0",
    "prettier": "^3.6.2",
    "rollup": "^4.46.0",
    "rollup-plugin-auto-external": "^2.0.0",
    "rollup-plugin-dts": "^6.2.1",
    "semantic-release": "^24.2.7",
    "typescript": "^5.8.3",
    "vitest": "^3.2.4"
  },
  "engines": {
    "node": ">=18.12.1",
    "npm": ">=8.19.2"
  }
}
